<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #3498db;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="themstore.Controller.ListeDesRéclamationsController">
   <children>
      <Label fx:id="labelliste" layoutX="50.0" layoutY="14.0" prefHeight="75.0" prefWidth="512.0" text="liste de mes réclamations" textFill="#de4545">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
      <Button fx:id="retour" cache="true" layoutX="464.0" layoutY="325.0" onAction="#handleButtonAction" prefHeight="56.0" prefWidth="129.0" style="-fx-background-color: #00FF00;" text="retour" textFill="RED">
         <effect>
            <Blend />
         </effect></Button>
      <TableView fx:id="table" layoutX="87.0" layoutY="72.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="subject" prefWidth="96.0" text="sujet" />
          <TableColumn fx:id="msg" prefWidth="103.0" text="message" />
        </columns>
      </TableView>
      <Button fx:id="delete" layoutX="300.0" layoutY="331.0" mnemonicParsing="false" onAction="#buttondelete" text="delete" />
   </children>
</AnchorPane>
